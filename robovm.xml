<config>
	<mainClass>com.thnkld.burungkepak.ios.FlxIOSApplication</mainClass>
	<os>ios</os>
	<arch>x86</arch>
	<iosInfoPList>Info.plist.xml</iosInfoPList>
	<libs>
		<lib>libs/gdx-ioslibs/libgdx.a</lib>
		<lib>libs/gdx-ioslibs/libObjectAL.a</lib>
		<lib>libs/gdx-ioslibs/libgdx-freetype.a</lib>
		<!-- google play game services -->
		<lib>playgameservices-ios/libs/GooglePlus.a</lib>
		<lib>playgameservices-ios/libs/GoogleOpenSource.a</lib>
		<lib>playgameservices-ios/libs/PlayGameServices.a</lib>
	</libs>
	<frameworks>
		<framework>AssetsLibrary</framework>
		<framework>AudioToolbox</framework>
		<framework>AVFoundation</framework>
		<framework>CoreData</framework>
		<framework>CoreGraphics</framework>
		<framework>CoreLocation</framework>
		<framework>CoreMotion</framework>
		<framework>CoreText</framework>
		<framework>MediaPlayer</framework>
		<framework>OpenAL</framework>
		<framework>OpenGLES</framework>
		<framework>QuartzCore</framework>
		<framework>Security</framework>
		<framework>SystemConfiguration</framework>
		<framework>UIKit</framework>
	</frameworks>
	<resources>
		<resource>
			<directory>assets</directory>
		</resource>
		<resource>
			<directory>burungkepak-robovm/data</directory>
		</resource>
		<resource>
			<directory>playgameservices-ios/resources</directory>
			<skipPngCrush>true</skipPngCrush>
		</resource>
	</resources>
	<classpath>
		<classpathentry>out/production/burungkepak-robovm</classpathentry>
		<classpathentry>out/production/burungkepak-core</classpathentry>
		<classpathentry>out/production/playgameservices-ios</classpathentry>
		<classpathentry>out/production/flixel-core</classpathentry>
		<classpathentry>libs/gdx-core/gdx.jar</classpathentry>
		<classpathentry>libs/gdx-freetype/gdx-freetype.jar</classpathentry>
		<classpathentry>libs/gdx-robovm/gdx-backend-robovm.jar</classpathentry>
		<classpathentry>libs/robovm-cocoatouch/robovm-cocoatouch.jar</classpathentry>
		<classpathentry>libs/robovm-rt/robovm-rt.jar</classpathentry>
		<classpathentry>libs/robovm-objc/robovm-objc.jar</classpathentry>
	</classpath>
</config>